{
  "name": "azure-omni-vui-personalization-sb-cmp",
  "private": true,
  "scripts": {
    "postinstall": "cp -n templates/appsettings.json.template settings/appsettings.json || true",
    "setup": "./scripts/setup.sh dev",
    "dev": "cross-env NODE_ENV=dev node index.js",
    "start": "node index.js",
    "build:schema": "cd schemas && sh ./update-schema.sh",
    "test": "node_modules/mocha/bin/mocha -r ts-node/register --extension ts --extension js --recursive __tests__",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test",
    "test:dialog": "node_modules/mocha/bin/mocha __spec__ --reporter mochawesome --reporter-options reportDir=botium-report,reportFilename=botium-report.html",
    "test:dialog-smoke": "TEST_SCOPE=smoke node_modules/mocha/bin/mocha __spec__ --reporter mochawesome --reporter-options reportDir=botium-report,reportFilename=botium-report.htm"
  },
  "botium": {
    "expandConvos": true,
    "expandUtterancesToConvos": false,
    "expandScriptingMemoryToConvos": false
  },
  "dependencies": {
    "@advanceddevelopment/omni-vui-custom-actions": "2.0.44",
    "@azure/data-tables": "^13.2.2",
    "agentkeepalive": "4.3.0",
    "ansi-regex": ">=5.0.1",
    "async": ">=2.6.4",
    "axios": ">=0.21.2",
    "axios-retry": "^3.4.0",
    "botbuilder-ai-luis": "4.14.1-preview",
    "botbuilder-ai-qna": "4.14.1-preview",
    "botbuilder-dialogs-adaptive-runtime": "4.14.1-preview",
    "botbuilder-dialogs-adaptive-runtime-integration-express": "4.14.1-preview",
    "cross-env": "latest",
    "cross-fetch": ">=3.1.5",
    "engine.io": ">=5.2.1",
    "json-schema": ">=0.4.0",
    "markdown-it": ">=12.3.2",
    "minimist": ">=1.2.6",
    "moment": ">=2.29.4",
    "nconf": ">=0.11.4",
    "swagger-ui-dist": ">=4.1.3",
    "url-parse": ">=1.5.9",
    "validator": ">=13.7.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/sinon": "^10.0.2",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "mocha": "^9.0.0",
    "mochawesome": "^6.2.2",
    "nyc": "^15.1.0",
    "sinon": "^11.1.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
