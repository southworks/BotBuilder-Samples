# PegName(param1,param2)
- IF: ${toLower(param1) == 'askdaterange'}
    - PolicySelectionSkill_${param1}_${param2}_Peg
- ELSEIF: ${toLower(param1) == 'askdos'}
    - PolicySelectionSkill_${param1}_${param2}_Peg 
- ELSEIF: ${toLower(param1) == 'asksupmed'}
    - PolicySelectionSkill_${param1}_${param2}_Peg 
- ELSEIF: ${toLower(param1) == 'asksupmedrx'}
    - PolicySelectionSkill_${param1}_${param2}_Peg
- ELSEIF: ${toLower(param1) == 'askdualpolicydynamicquestion'}
