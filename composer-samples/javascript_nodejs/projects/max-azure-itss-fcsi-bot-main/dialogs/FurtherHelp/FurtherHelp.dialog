            },
            {
              "$kind": "Microsoft.BeginDialog",
              "$designer": {
                "id": "9cS1Hh"
              },
              "activityProcessed": true,
              "dialog": "Feedback_flow"
            }
          ],
          "actions": [
            {
              "$kind": "Microsoft.ConfirmInput",
              "$designer": {
                "id": "k2AuEZ"
              },
              "defaultLocale": "en-us",
