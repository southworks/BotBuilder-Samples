        {
          "$kind": "SendEventTelemetry",
          "$designer": {
            "id": "pinFkV"
          },
          "properties": "{\"pathTraversed\" : `${conversation.pathTaken}`}",
          "name": "user-path"
        },
        {
          "$kind": "Microsoft.IfCondition",
