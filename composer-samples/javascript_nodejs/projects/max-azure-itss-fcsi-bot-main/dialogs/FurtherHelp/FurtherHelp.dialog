                },
                {
                  "$kind": "Microsoft.BeginDialog",
                  "$designer": {
                    "id": "GR696J"
                  },
                  "activityProcessed": true,
                  "dialog": "Feedback_flow"
                }
              ],
              "actions": [
                {
                  "$kind": "SendEventTelemetry",
                  "$designer": {
                    "id": "WX8RSy"
                  },
                  "name": "need-further-help",
                  "properties": "{ \"answer\" : \"yes\"}"
                },
                {
                  "$kind": "Microsoft.SetProperty",
                  "$designer": {
                    "id": "GHHxmN"
                  },
                  "property": "conversation.helpShow",
