                        },
                        {
                          "property": "conversation.DMText",
                          "value": "=getProperty(conversation.DMData.OPM, concat(\"TTS_Prompt\",string(dialog.foreach.index+1)))"
                        },
                        {
                          "property": "conversation.DMbargeIn",
                          "value": "=getProperty(conversation.DMData.OPM, concat(\"Bargein_Prompt\",string(dialog.foreach.index+1)))==\"true\""
                        }
                      ]
                    },
                    {
                      "$kind": "Microsoft.IfCondition",
                      "$designer": {
                        "id": "gLchQL"
                      },
