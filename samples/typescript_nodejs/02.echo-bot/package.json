{
    "name": "echobot",
    "version": "1.0.0",
    "description": "Bot Builder v4 echo bot sample",
    "author": "Microsoft",
    "license": "MIT",
    "main": "./lib/index.js",
    "scripts": {
        "build": "tsc --build",
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "postinstall": "npm run build && node ./deploymentScripts/webConfigPrep.js",
        "p1": "set HTTPS_PROXY=143.110.190.83:8080",
        "p2": "set HTTP_PROXY=143.110.190.83:8080",
        "p3": "set no_proxy=localhost",
        "start": "tsc --build && node ./lib/index.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "watch": "nodemon --watch ./src -e ts --exec \"npm run start\""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com"
    },
    "resolutions": {
        "@azure/msal-common": "7.6.0"
    },
    "dependencies": {
        "botbuilder": "4.21.0",
        "dotenv": "^8.2.0",
        "replace": "^1.2.0",
        "restify": "^10.0.0"
    },
    "devDependencies": {
        "@types/dotenv": "6.1.1",
        "@types/node": "^16.11.6",
        "@types/restify": "8.4.2",
        "nodemon": "~2.0.4",
        "tslint": "~6.1.2",
        "typescript": "~4.3.2"
    }
}
